    <div class="container">
        <div class="row">

            <table class="table">
            <thead class="thead-dark">
                <tr>
                <th scope="col">#</th>
                <th scope="col">Nama</th>
                <th scope="col">Email</th>
                <th scope="col">No. HP</th>
                <th scope="col">Aksi</th>
                </tr>
            </thead>
            <tbody>
                <% if (contacts.length === 0) { %>
                   <tr>
                    <td colspan="4">Tidak ada  Data </td>
                   </tr> 
                <% } %> 
                <% contacts.forEach((contact,i) => { i++ %>
                <tr>
                    <th scope="row"> <%= i %> </th>
                    <td><%= contact.nama  %> </td>
                    <td><%= contact.email  %> </td>
                    <td><%= contact.nohp       %> </td>
                    <td><a href="/contact/<%= contact.nama %>" class="badge badge-success rounded-pill">Lihat Detail</td>
                </tr>
                 
                <% }) %>

            </tbody>
            </table>                        
        </div>
    </div>
